{
    "name": "Tsiunim Speaker",
    "description": "write tsiunim in pages by voice.",
    "version": "1.0",
    "browser_action": {
        "default_title": "Tsiunim Speaker"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "unlimitedStorage",
        "browsingData",
        "notifications",
        "http://*/*",
        "https://*/*",
        "<all_urls>",
        "background"
    ],
    "omnibox": {
        "keyword": "tsiunim"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "jquery-1.10.2.min.js",
                "example.js",
            ],
            "css": [
                "alef.css"
            ]
        }
    ],
    "manifest_version": 2
}